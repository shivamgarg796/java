package com.assignment;

public class CricketScore {
//	static sum;
//	
	static void score(int arr[]) {
		for(int i=0;i<arr.length;i++) {
			System.out.println(arr[i]);
		}
	}
//		return sum;
	
	
	public static void main(String[] args) {
		int arr[]= {1,4,6,1,2,3};
//		System.out.println(arr);
		score(arr);
	}

}
